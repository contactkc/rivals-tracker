spring.application.name=backend
spring.security.enabled=false
spring.datasource.url=jdbc:postgresql://localhost:5432/marvelrivalsdb
spring.datasource.username=postgres
spring.datasource.password=passwordhere
spring.datasource.driver-class-name=org.postgresql.Driver
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
rivals.api.key=${RIVALS_API_KEY}

# JWT config
jwt.secret=your-256-bit-secret-key-should-be-at-least-32-characters
jwt.expiration=86400000